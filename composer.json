{
    "name": "iprospect_ca/newproject",
    "type": "project",
    "license": "MIT",
    "description": "Blank Strata Template",
    "homepage": "https://strata.francoisfaubert.com",
    "authors": [
        {
            "name": "Francois Faubert",
            "email": "frank@francoisfaubert.com",
            "homepage": "http://www.francoisfaubert.com"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "repositories": [{
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.4",
        "vlucas/phpdotenv": "^2.0.1",
        "johnpbloch/wordpress": "4.2.3",
        "francoisfaubert/strata": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    }
}
