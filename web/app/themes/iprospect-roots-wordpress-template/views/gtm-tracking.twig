{% if events | length == 0 %}
{% else %}
	<script type='text/javascript'>
	{% for j, event in events %}
		trackGTMAction('{{ event.category }}', '{{ event.action }}', '{{ event.label }}');
	{% endfor %}

	function trackGTMAction (category, action, label){
		alert([category, action, label].join(' | '));
		try{
	        dataLayer.push({
	            'eventCategory': category,
	            'eventAction': action,
	            'eventLabel': label,
	            'event': 'gtm_event'
	        });

		}catch(e){
			//console.log(e);
		}
	}
	</script>
{% endif %}